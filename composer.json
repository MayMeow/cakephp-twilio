{
    "name": "maymeow/cakephp-twilio",
    "description": "CakephpTwilio plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0",
        "twilio/sdk": "^6.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "CakephpTwilio\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakephpTwilio\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    }
}
